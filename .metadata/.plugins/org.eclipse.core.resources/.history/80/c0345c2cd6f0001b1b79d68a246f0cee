package step1;

public class TestReview {
	public static void main(String[] args) {
		//Market 객체 생성
		Market m = new Market();
		System.out.println(m.name); //null이 출력. 정보가 존재하지 않을 떄 null출력
		
		m.name = "G market"; //객체의 name 변수에 정보할당
		System.out.println(m.name); //G market
		
		String searchResult = m.searchProduct("라면");
		System.out.println("검색결과 : "searchResult);
	}
}
